{
  "name": "@zero-tech/zui",
  "version": "0.3.0",
  "description": "Zero UI React Component Library",
  "scripts": {
    "build": "rm -rf dist && yarn build:typecheck && yarn lint && yarn test && yarn build:styles && yarn export:styles && yarn build:scripts && cp package.json dist/package.json",
    "build:only": "yarn build-styles && yarn build:scripts",
    "build:scripts": "NODE_ENV=production babel --extensions \".ts,.tsx,.js,.jsx\" ./src --out-dir ./dist",
    "build:styles": "node-sass ./src -o dist/ --importer ./node_modules/node-sass-import",
    "build:typecheck": "tsc --project ./tsconfig.json",
    "export:styles": "node .scripts/exportStyles.js",
    "generate": "node ./util/create-component",
    "lint": "yarn lint:scripts && yarn lint:styles && yarn prettify",
    "lint:scripts": "eslint --ext .ts,.tsx,.js,.jsx ./ --fix --max-warnings 1",
    "lint:styles": "stylelint -q \"./**/*.{css,scss}\" --max-warnings 1",
    "prettify": "prettier -w ./",
    "publish:components": "cd ./dist && npm publish",
    "publish:storybook": "NODE_ENV=storybook-production build-storybook -o dist-storybook",
    "storybook": "start-storybook -p 6007",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-export-default-from": "^7.16.0",
    "@babel/plugin-proposal-function-bind": "^7.16.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.16.0",
    "@babel/plugin-transform-destructuring": "^7.16.0",
    "@babel/plugin-transform-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addons": "^6.4.4",
    "@storybook/builder-webpack5": "^6.4.4",
    "@storybook/manager-webpack5": "^6.4.4",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/babel__core": "^7.1.18",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-plugin-react-docgen": "^4.2.1",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "node-sass": "^6.0.1",
    "node-sass-import": "^2.0.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^4.0.0",
    "ts-jest": "^27.1.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.4.2",
    "webpack": "^5.73.0"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@radix-ui/react-accessible-icon": "^1.0.0",
    "@radix-ui/react-aspect-ratio": "^0.1.4",
    "@radix-ui/react-dialog": "^0.1.7",
    "@radix-ui/react-dropdown-menu": "^0.1.6",
    "@radix-ui/react-tabs": "^0.1.5",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@react-aria/button": "^3.5.0",
    "@react-aria/i18n": "^3.4.1",
    "@react-aria/numberfield": "^3.2.1",
    "@react-aria/textfield": "^3.6.1",
    "@react-stately/numberfield": "^3.1.1",
    "@stitches/react": "^1.2.8",
    "@storybook/preset-scss": "^1.0.3",
    "classnames": "^2.3.1",
    "ethers": "^5.6.9",
    "focus-visible": "^5.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.52.2"
  }
}
